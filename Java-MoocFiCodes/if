import java.util.Scanner;

public class Main {

   public static void main(String[] args) {
       Scanner scanner = new Scanner(System.in);
       double score = scanner.nextDouble();
       if (score > 3.5) {
       	      	System.out.println("Congratulations");
       } else {
       	System.out.println("Sorry");
       }// your code goes here
       
   }
}
